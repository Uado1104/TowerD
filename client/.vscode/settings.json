{
  "eslint.validate": [
    "typescript"
  ],
  "jestrunner.codeLensSelector": "**/*.{test,spec,e2e-spec}.{js,jsx,ts,tsx}",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "jestrunner.debugOptions": {
    "args": [
      "--testTimeout=1000000"
    ],
    "env": {
        "NODE_ENV": "test"
    },
  },
  "editor.tabSize": 4,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/dist": true,
    "**/build": true,
    "**/node_modules": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#5a507a",
		"activityBar.background": "#5a507a",
		"activityBar.foreground": "#e7e7e7",
		"activityBar.inactiveForeground": "#e7e7e799",
		"activityBarBadge.background": "#a78378",
		"activityBarBadge.foreground": "#15202b",
		"commandCenter.border": "#e7e7e799",
		"sash.hoverBorder": "#5a507a",
		"statusBar.background": "#433c5b",
		"statusBar.foreground": "#e7e7e7",
		"statusBarItem.hoverBackground": "#5a507a",
		"statusBarItem.remoteBackground": "#433c5b",
		"statusBarItem.remoteForeground": "#e7e7e7",
		"titleBar.activeBackground": "#433c5b",
		"titleBar.activeForeground": "#e7e7e7",
		"titleBar.inactiveBackground": "#433c5b99",
		"titleBar.inactiveForeground": "#e7e7e799"
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.tsdk": "node_modules/typescript/lib"
}